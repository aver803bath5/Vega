{
    "ConnectionStrings": {
        "Default": "server=localhost; database=vega; user id=sa; password=yourStrong(!)Password"
    },
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning",
            "Microsoft.Hosting.Lifetime": "Information"
        }
    },
    "Kestrel": {
        "Endpoints": {
            "Http": {
                "Url": "[URL you want to bind in the docker (normally will be 0.0.0.0:PortNumber)]"
            },
            "Https": {
                "Url": "[URL you want to bind in the docker (normally will be 0.0.0.0:PortNumber)]"
            }
        }
    },
    "StoredFilePath": "[Directory to store the vehicle photos files]",
    "RequestFilePath": "/uploads",
    "FileSizeLimit": 2097152,
    "Auth0": {
        "Domain": "[Auth0 tenant domain]",
        "Audience": "[Auth0 API identifier]"
    }
}
